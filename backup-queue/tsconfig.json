{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "lib": ["ES2020"],
      "types": ["@cloudflare/workers-types", "node"],
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  